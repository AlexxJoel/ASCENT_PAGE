---
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import SideMenu from "../components/SideMenu.astro";
interface Props {
    title: string
}

const {title} = Astro.props
---

<!doctype html>
<html lang="es">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <meta name="description"
          content="Ascent es una empresa de servicios de consultoría en tecnología de la información y desarrollo de software."/>
    <meta name="author" content="Ascent"/>
    <meta name="keywords" content="Ascent, Consultoría, Tecnología, Desarrollo, Software"/>
    <meta name="robots" content="index, follow"/>
    <meta name="generator" content={Astro.generator}/>
    <title>{title}</title>
</head>
<body>
<Header/>
<main>
    <slot/>
</main>
<Footer/>
</body>





